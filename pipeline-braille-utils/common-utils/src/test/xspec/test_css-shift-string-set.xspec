<?xml version="1.0" encoding="utf-8"?>
<x:description xmlns:x="http://www.jenitennison.com/xslt/xspec"
               xmlns:css="http://www.daisy.org/ns/pipeline/braille-css"
               stylesheet="../../main/resources/xml/formatting/css-shift-string-set.xsl">
  
  <x:scenario label="test_01">
    <x:context>
      <css:root>
        <css:_ css:string-set="foo 'x'"/>
        <css:box css:string-entry="bar 'y'"/>
      </css:root>
    </x:context>
    <x:expect label="result">
      <css:root>
        <css:_/>
        <css:box css:string-entry="foo 'x', bar 'y'"/>
      </css:root>
    </x:expect>
  </x:scenario>
  
</x:description>
